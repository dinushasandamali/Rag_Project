{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "module": "esnext",
    "module": "esnext",
    "strict": true,
    "jsx": "react-native",
    "target": "esnext",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": [
    "app/**/*.ts",
    "app/**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
, "styles/styles.ts"  ]
}
